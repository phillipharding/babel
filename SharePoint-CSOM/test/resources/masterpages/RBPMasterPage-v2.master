							SourceUrl: window.location.href
						},
						dialogReturnValueCallback: onSubmitFeedbackDialogClosed
					};
					SP.UI.ModalDialog.showModalDialog(dialogOptions);
				};
				function onClickSubmitFeedback(e) {
					e.preventDefault();
					var dialogOptions = {
						url: "/Style Library/RBPv2/Apps/Page Feedback/page-feedback.aspx",
						title: "Report a Page for Inappropriate Content.",
						width: 600,
						height: 260,
						showClose: true,
						allowMaximize: false,
						autoSize: true,
						args: {
							SourceUrl: window.location.href
						},
						dialogReturnValueCallback: onSubmitFeedbackDialogClosed
					};
					SP.UI.ModalDialog.showModalDialog(dialogOptions);
				};
				function onSubmitFeedbackDialogClosed(dialogResult, returnValue) {
					/* call OOTB function with dialog result */
					/* RefreshPage(dialogResult); */
				}
				
				var megaConfig = {
					 interval: 350,
					 sensitivity: 4,
					 over: addMega,
					 timeout: 350,
					 out: removeMega
				};

				$(document).ready(function() {
					$("li.hasMega").hoverIntent(megaConfig);
					_pels.btnReport = $("#btnSubmitFeedback");
					_pels.btnReport.click(onClickSubmitFeedback);
					_pels.btnTermsAndConditions = $('#btnSubmitDisclaimer');
					_pels.btnTermsAndConditions.click(onClickViewTermsAndConditions);
					
				});

				function addMega(){
					$(this).addClass("hovering");
				}

				function removeMega(){
					$(this).removeClass("hovering");
				}
		</script>
