			<UserCustomAction Scope="Site" DOMElement="body">
				<Name>Inject-Site-Javascript-Block1</Name>
				<Location>ScriptLink</Location>
				<Sequence>101</Sequence>
				<ScriptBlock>
(function() {
	function jQueryLoaded() {
		/*alert('jQuery is Loaded...');*/
		var $e = $('<a title="jQuery has been loaded on the page">jQuery is Loaded...</a>')
						.attr('href','#')
						.css({ 'color':'white','margin-left':'10px','background-color':'rgb(0, 114, 198)','font-weight': 800,'font-size':'10pt','text-align':'center'})
						.addClass('ms-core-listMenu-item ms-core-listMenu-heading');
		$('#ctl00_PlaceHolderLeftNavBar_PlaceHolderQuickLaunchBottom_idNavLinkViewAll').after($e);
	}
	_spBodyOnLoadFunctions.push(function() {
		ExecuteOrDelayUntilScriptLoaded(function() { 
			$(jQueryLoaded);
		}, 'SP.core.js');
	});
})();
				</ScriptBlock>
